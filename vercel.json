{
    "buildCommand": "npm run build",
    "installCommand" : "npm install",
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "files.exclude": [
        "**/*.test.ts",
        "**/node_modules",
        "**/.gitignore",
        "**/.git"
    ]
}